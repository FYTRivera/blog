<%= render "shared/navbar" %>
<div class="wrapper">
    <%= form_with(model: @article) do |f| %>

    <p>
        <%= f.label :name %><br/>
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.label :body %><br/>
        <%= f.text_area :body %>
    </p>

    <p>
        <%= f.submit "Save", class: "button is-primary"%>
    </p>

    <% end %>
</div>